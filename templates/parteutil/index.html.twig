{% extends 'base.html.twig' %}

{% block title %}Parte útil{% endblock %}

{% block body %}
    <h1>Parte útil</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Idparteutil</th>
                <th>Parte</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for parteutil in parteutils %}
            <tr>
                <td>{{ parteutil.idparteutil }}</td>
                <td>{{ parteutil.parte }}</td>
                <td>
                    <a href="{{ path('parteutil_show', {'idparteutil': parteutil.idparteutil}) }}">show</a>
                    <a href="{{ path('parteutil_edit', {'idparteutil': parteutil.idparteutil}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('parteutil_new') }}">Crear nueva</a>
{% endblock %}
